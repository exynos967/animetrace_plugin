{
  "manifest_version": 1,
  "name": "AnimeTrace 图片识别插件",
  "version": "2.2.0",
  "description": "接收图片并调用 AnimeTrace 识别，返回文本结果到 QQ",
  "author": {
    "name": "薄暝&晴空",
    "url": "https://github.com/exynos967/animetrace_plugin"
  },
  "license": "MIT",

  "host_application": {
    "min_version": "0.8.0"
  },
  "homepage_url": "https://github.com/exynos967/animetrace_plugin",
  "repository_url": "https://github.com/exynos967/animetrace_plugin",
  "keywords": ["anime", "trace", "image", "recognition", "搜番"],
  "categories": ["Multimedia", "External Integration"],

  "default_locale": "zh-CN",
  "locales_path": "_locales",

  "plugin_info": {
    "is_built_in": false,
    "plugin_type": "service",
    "components": [
      {
        "type": "tool",
        "name": "anime_trace_search",
        "description": "AnimeTrace image search tool"
      }
    ]
  }
}
