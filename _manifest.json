{
  "manifest_version": 1,
  "name": "AnimeTrace 图片识别插件 (AnimeTrace Image Recognizer)",
  "version": "1.0.0",
  "description": "接收图片并调用 AnimeTrace 识别，返回文本结果到 QQ",
  "author": {
    "name": "MaiBot 插件开发",
    "url": "https://github.com/MaiM-with-u"
  },
  "license": "GPL-v3.0-or-later",

  "host_application": {
    "min_version": "0.8.0"
  },
  "homepage_url": "https://github.com/MaiM-with-u/maibot",
  "repository_url": "https://github.com/MaiM-with-u/maibot",
  "keywords": ["anime", "trace", "image", "recognition", "搜番"],
  "categories": ["Image", "Recognition", "Utility"],

  "default_locale": "zh-CN",
  "locales_path": "_locales",

  "plugin_info": {
    "is_built_in": false,
    "plugin_type": "service",
    "components": [
      {
        "type": "event_handler",
        "name": "animetrace_on_message",
        "description": "接收图片并识别"
      }
    ]
  }
}

